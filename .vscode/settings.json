{
    "go.buildTags": "dev",
    "go.formatTool": "goimports",
    "go.lintTool":"golangci-lint",
    "go.lintOnSave": "package",
    "go.lintFlags": [
      "--fast"
    ],
    "go.vetOnSave": "off",
    "go.testFlags": [
        "-v",
        "-race",
        "-failfast",
        "-cpuprofile", "profile.out",
    ],
    "go.testTimeout": "60s",
    "go.testEnvVars": {
        "DEBUG_BLOCKS": "false",
        "LOG_LEVEL": "debug",
        "BIND_LOCAL": "true",
        "GO111MODULE": "on"
    },
    "terminal.integrated.env.osx": {
        "BIND_LOCAL": "true",
        "GO111MODULE": "on"
    },
    "terminal.integrated.env.windows": {
        "BIND_LOCAL": "true",
        "GO111MODULE": "on"
    },
    "terminal.integrated.env.linux": {
        "BIND_LOCAL": "true",
        "GO111MODULE": "on"
    },
    "editor.rulers": [
        80,
    ],
}