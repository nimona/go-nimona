{
    "go.buildTags": "dev",
    "go.testEnvVars": {
        "GOMAXPROCS": "1",
        "NIMONA_LOG_LEVEL": "DEBUG"
    },
    "go.testFlags": [
        "-v",
        "--race",
        "--failfast",
        "--count=1",
    ],
    "go.useLanguageServer": true,
    "go.testTimeout": "60s",
    "gopls": {
        "build.experimentalWorkspaceModule": true,
        "formatting.gofumpt": true,
    },
    "go.languageServerExperimentalFeatures": {
    },
    "editor.rulers": [
        80,
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.pyenv": true,
        "**/.terraform": true,
        "**/node_modules": true,
        "**/_gen": true,
        "examples": true,
        "bin": true,
    },
    "search.exclude": {
        "**/.git": true,
        "**/.pyenv": true,
        "**/.terraform": true,
        "**/node_modules": true,
        "**/_gen": true,
        "examples": true,
        "bin": true,
    },
}