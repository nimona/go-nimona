{
    "go.buildTags": "dev",
    "go.testFlags": [
        "-v",
        "--race",
    ],
    "go.useLanguageServer": true,
    "go.testTimeout": "60s",
    "gopls": {
        "build.experimentalWorkspaceModule": true,
        "formatting.gofumpt": true,
        "ui.completion.completionBudget": "50ms",
        "ui.completion.matcher": "Fuzzy",
        "ui.completion.usePlaceholders": true,
    },
    "go.languageServerExperimentalFeatures": {
    },
    "editor.rulers": [
        80,
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.pyenv": true,
        "**/.terraform": true,
        "**/node_modules": true,
        "**/_gen": true,
        "examples": true,
        "bin": true,
    },
    "search.exclude": {
        "**/.git": true,
        "**/.pyenv": true,
        "**/.terraform": true,
        "**/node_modules": true,
        "**/_gen": true,
        "examples": true,
        "bin": true,
    },
}