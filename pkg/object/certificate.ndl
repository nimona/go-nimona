package nimona.io/object

signed object nimona.io/object.Certificate {
    nonce string
    description string
    subjects repeated string type=nimona.io/crypto.PublicKey
    created string
    expires string
}

object nimona.io/object.CertificateRequest {
    applicationName string
    applicationDescription string
    applicationURL string
    subject string
    resources repeated string
    actions repeated string
    nonce string
}
