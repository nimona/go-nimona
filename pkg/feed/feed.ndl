package nimona.io/feed

import nimona.io/crypto crypto
import nimona.io/object object

stream nimona.io/feed {
    signed root event Created {
        type string
        datetime string
    }
    signed event Added {
        objectHash repeated nimona.io/object.Hash
        sequence int
        datetime string
    }
    signed event Removed {
        objectHash repeated nimona.io/object.Hash
        sequence int
        datetime string
    }
}
